  title My Emergency
header
  nav.navigation
    .nav-wrapper
      a.brand-logo(href='#') Logo
      a.sidenav-trigger.left(href='#' data-target='slide-out')
        i.material-icons menu
      ul#nav-mobile.right.hide-on-med-and-down
        li
          a(href='index.html') Home
        li
          a(href='emergencies.html') Emergencies
        li
          a(href='#') Login
        li
          a(href='#') Sign Up
        li
          a(href='account.html')
            .navigation__user
              img.circle(src='img/user/default.jpg')
        li
          a(href='#') Log Out
  ul#slide-out.sidenav
    li
      .user-view
        .background
          img(src='img/black.jpg')
        a(href='#user')
          img.circle(src='img/user/default.jpg')
        a(href='#name')
          span.white-text.name John Doe
        a(href='#email')
          span.white-text.email jdandturk@gmail.com
    li
      a.waves-effect(href='index.html') Home
    li
      a.waves-effect(href='emergencies.html') Emergencies
    li
      a.waves-effect(href='#') Login
    li
      a.waves-effect(href='#') Sign Up
    li
      a.waves-effect(href='#') Log Out
